<% provide(:title, @user.name) %>
<div class="row">
    <asside class="col-md-4">
        <section class="user_info">
            <h1>
                <%= gravatar_for @user %>
                <%= @user.name %>
            </h1>
            <br/>
            <p><b>Email: </b><%= @user.email %></p>
            <p><b>Created at: </b><%= @user.created_at %></p>
            <p><b>Updated at: </b><%= @user.updated_at %></p>
        </section>
    </aside>
</div>
<%= Time.now %>
